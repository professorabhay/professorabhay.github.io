{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "ignorePatterns": [
    "_site/**/*.wasm.js",
    "_site/**/*.wasmmodule.js",
    "_site/archive/",
    "_site/**/build/*",
    "_site/**/coi-serviceworker.js"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "require-jsdoc": 0
  }
}
